{
  "compilerOptions": {
    "outDir": "build",
    "forceConsistentCasingInFileNames": true,
    "declarationMap": true,
    "sourceMap": true,
    "alwaysStrict": true,
    "declaration": true,
    "experimentalDecorators": true,
    "incremental": true,
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": false,
    "target": "es2020",
    "composite": false,
    "tsBuildInfoFile": "build/tsconfig.tsbuildinfo"
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "_incoming/**/*",
    "**/*.spec.ts",
    "jest.config.ts",
    "build/**/*.ts"
  ],
  "_generated_by_jsii_": "Generated by jsii - safe to delete, and ideally should be in .gitignore"
}